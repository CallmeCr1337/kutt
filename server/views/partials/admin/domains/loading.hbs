{{#unless table_domains}}
  {{#ifEquals table_domains.length 0}}
    <tr class="no-data">
      <td>
        {{t "table.admin.noDomains"}}
      </td>
    </tr>
  {{else}}
    <tr class="loading-placeholder">
      <td>
        {{> icons/spinner}}
        {{t "table.admin.loadingDomains"}}
      </td>
    </tr>
  {{/ifEquals}}
{{/unless}}