<thead>
  <tr class="controls links-controls">
    <th class="search">
      <input class="table-input search" id="search" name="search" type="text" placeholder="{{__ "search"}}" hx-on:keyup="resetTableNav()" />
      <input id="total" name="total" type="hidden" value="{{total}}" />
      <input id="limit" name="limit" type="hidden" value="10" />
      <input id="skip" name="skip" type="hidden" value="0" />
    </th>
    {{> links/nav}}
  </tr>
  <tr>
    <th class="original-url">{{__ "originalUrl"}}</th>
    <th class="created-at">{{__ "createdAt"}}</th>
    <th class="short-link">{{__ "shortLink"}}</th>
    <th class="views">{{__ "views"}}</th>
    <th class="actions"></th>
  </tr>
</thead>